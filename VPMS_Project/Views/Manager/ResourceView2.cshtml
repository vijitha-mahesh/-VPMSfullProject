@model List<Employee>
    <div class="col-11 pt-4">
        <div class="row " style="background-color: #464a4e; border-radius: 6px; padding: 3px;  margin-bottom: 30px;">
            <div class="col-2">

                <label class="text-white">ID</label>
            </div>
            <div class="col-5">
                <label class="text-white">NAME</label>

            </div>
            <div class="col-5">
                <label class="text-white">DESIGNATION</label>

            </div>

        </div>


        @foreach (var emp in Model)
        {
            <div class="row " style="background-color:#88804c; border-radius: 6px; padding: 3px;  margin-bottom: 18px;">
                <div class="col-2">

                    <label class="text-white">@emp.Id</label>
                </div>
                <div class="col-5">
                    <label class="text-white text-capitalize">@emp.Name</label>

                </div>
                <div class="col-5">
                    <label class="text-white text-capitalize">@emp.Designation</label>

                </div>

            </div>
        }
    </div>